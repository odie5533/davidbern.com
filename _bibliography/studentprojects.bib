	date-modified = {2022-04-29 16:24:54 +0200},
	keywords = {Action video games, MOBA, cognition, perception, attention, reaction time, visual search},
	month = {09},
	school = {Zurich University of Applied Sciences (ZHAW)},
	selected = {true},
	title = {More Than Just Fun and Games? An Investigation of the Link Between Video Game Play and Cognitive, Perceptual, and Attentional Skills},
	type = {Bachelor's Thesis},
